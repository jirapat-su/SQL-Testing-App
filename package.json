{
  "name": "sql-testing-app",
  "version": "1.1.0",
  "private": true,
  "prisma": {
    "schema": "./database",
    "seed": "bun ./database/seeds/index.ts"
  },
  "packageManager": "bun@1.2.0",
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.2.0"
  },
  "scripts": {
    "prepare": "bun .husky/install.mjs",
    "postinstall": "bun db:generate",
    "commit": "cz",
    "_____________app_scripts_____________": "",
    "dev": "dotenvx run -f $(bun run env:file) -- next dev --turbopack",
    "build": "dotenvx run -f $(bun run env:file) -- next build",
    "start": "dotenvx run -f $(bun run env:file) -- next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "clean": "rm -rf dist .vercel node_modules/.tmp node_modules/.cache",
    "clean:deps": "rm -rf node_modules bun.lockb bun.lock",
    "___________dotenvx_scripts____________": "",
    "env:file": "echo $([ \"$VERCEL_ENV\" = \"production\" ] && echo \".env.production\" || echo \".env\")",
    "env:ec": "dotenvx encrypt -f .env && dotenvx encrypt -f .env.production",
    "env:dc": "dotenvx decrypt -f .env && dotenvx decrypt -f .env.production",
    "____________prisma_scripts____________": "",
    "db:validate": "dotenvx run -f .env -- prisma validate",
    "db:generate": "dotenvx run -f .env -- prisma generate --no-engine",
    "db:migrate": "dotenvx run -f .env -- prisma migrate dev --skip-seed",
    "db:reset": "dotenvx run -f .env -- prisma migrate reset --force",
    "db:seed": "dotenvx run -f .env -- prisma db seed",
    "db:studio": "dotenvx run -f .env -- prisma studio",
    "__________better-auth_scripts_________": "",
    "auth:secret": "bunx @better-auth/cli@latest secret",
    "auth:generate": "dotenvx run -f .env -- bunx @better-auth/cli generate --config ./src/libs/better-auth/auth.ts",
    "_____________deps_scripts_____________": "",
    "check-deps": "bunx taze -r",
    "upgrade-deps": "bunx taze -r --interactive"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.3.3",
    "@elysiajs/eden": "^1.3.2",
    "@elysiajs/swagger": "^1.3.1",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/sarabun": "^5.2.6",
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "@mui/material-nextjs": "^7.2.0",
    "@mui/x-data-grid": "^8.8.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-query": "^5.83.0",
    "@toolpad/core": "^0.16.0",
    "@vercel/functions": "^2.2.3",
    "better-auth": "^1.2.12",
    "date-fns": "^4.1.0",
    "effect": "^3.16.13",
    "elysia": "^1.3.5",
    "keyv": "^5.3.4",
    "mysql2": "^3.14.2",
    "next": "15.3.5",
    "react": "^19.1.0",
    "react-big-calendar": "^1.19.4",
    "react-dom": "^19.1.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.17.0",
    "@dotenvx/dotenvx": "^1.47.6",
    "@eslint-react/eslint-plugin": "^1.52.3",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^20.19.8",
    "@types/react": "^19.1.8",
    "@types/react-big-calendar": "^1.16.2",
    "@types/react-dom": "^19.1.6",
    "commitizen": "^4.3.1",
    "commitlint": "^19.8.1",
    "commitlint-config-gitmoji": "^2.3.1",
    "cross-env": "^7.0.3",
    "cz-customizable": "^7.4.0",
    "eslint": "^9.31.0",
    "eslint-config-next": "15.3.5",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "husky": "^9.1.7",
    "prisma": "^6.12.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./cz.config.cjs"
    }
  }
}
