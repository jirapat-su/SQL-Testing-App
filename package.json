{
  "name": "sql-testing-app",
  "type": "module",
  "private": true,
  "prisma": {
    "schema": "./database/app",
    "seed": "bun ./database/app/seeds/index.ts"
  },
  "packageManager": "bun@1.2.0",
  "engines": {
    "node": ">=20.0.0",
    "bun": ">=1.2.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./cz.config.cjs"
    }
  },
  "scripts": {
    "prepare": "bun .husky/install.mjs",
    "postinstall": "bun run db:generate",
    "commit": "cz",
    "_____________app_scripts_____________": "",
    "dev": "dotenvx run -f $(bun run env:file) -- vike dev --host",
    "build": "dotenvx run -f $(bun run env:file) -- vike build",
    "preview": "cross-env NODE_ENV=production dotenvx run -f .env -- bun ./elysia-entry.node.ts",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --max-warnings=0 --fix",
    "___________dotenvx_scripts____________": "",
    "env:file": "echo $([ \"$VERCEL_ENV\" = \"production\" ] && echo \".env.production\" || echo \".env\")",
    "env:ec": "dotenvx encrypt -f .env && dotenvx encrypt -f .env.production",
    "env:dc": "dotenvx decrypt -f .env && dotenvx decrypt -f .env.production",
    "____________prisma_scripts____________": "",
    "db:validate": "dotenvx run -f .env -- prisma validate",
    "db:generate": "dotenvx run -f .env -- prisma generate --no-engine",
    "db:migrate": "dotenvx run -f .env -- prisma migrate dev --skip-seed",
    "db:reset": "dotenvx run -f .env -- prisma migrate reset --force",
    "db:seed": "dotenvx run -f .env -- prisma db seed",
    "db:studio": "dotenvx run -f .env -- prisma studio",
    "__________betterauth_scripts_________": "",
    "auth:secret": "bunx @better-auth/cli@latest secret",
    "auth:generate": "dotenvx run -f .env -- bunx @better-auth/cli generate --config ./src/libs/better-auth/auth.ts",
    "_____________deps_scripts_____________": "",
    "check-deps": "bunx taze -r",
    "upgrade-deps": "bunx taze -r --interactive"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.3.3",
    "@elysiajs/static": "^1.3.0",
    "@elysiajs/swagger": "^1.3.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/sarabun": "^5.2.6",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@prisma/extension-accelerate": "^2.0.1",
    "@sinclair/typemap": "^0.10.0",
    "@t3-oss/env-core": "^0.13.8",
    "@toolpad/core": "^0.16.0",
    "@universal-middleware/core": "^0.4.8",
    "@universal-middleware/elysia": "^0.5.2",
    "@vercel/functions": "^2.2.0",
    "@vite-plugin-vercel/vike": "^9.0.6",
    "@vitejs/plugin-react": "^4.5.2",
    "better-auth": "^1.2.9",
    "elysia": "^1.3.5",
    "keyv": "^5.3.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "vike": "^0.4.232",
    "vike-react": "^0.6.4",
    "vike-react-zustand": "^0.1.0",
    "vite-plugin-vercel": "^9.0.7",
    "zod": "^3.25.67",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.14.1",
    "@dotenvx/dotenvx": "^1.44.2",
    "@eslint-react/eslint-plugin": "^1.52.2",
    "@hono/vite-dev-server": "^0.19.1",
    "@tailwindcss/vite": "^4.1.10",
    "@types/node": "^20.19.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "commitizen": "^4.3.1",
    "commitlint": "^19.8.1",
    "commitlint-config-gitmoji": "^2.3.1",
    "cross-env": "^7.0.3",
    "cz-customizable": "^7.4.0",
    "eslint": "^9.29.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-perfectionist": "^4.14.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "husky": "^9.1.7",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prisma": "^6.9.0",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  }
}
